{
  "version": 1,
  "project_type": "code",
  "queue": {
    "file": ".ralph/queue.json",
    "done_file": ".ralph/done.json",
    "id_prefix": "RQ",
    "id_width": 4
  },
  "agent": {
    "runner": "opencode",
    "model": "kimi-for-coding/k2p5",
    "phases": 3,
    "iterations": 1,
    "repoprompt_plan_required": false,
    "repoprompt_tool_injection": false,
    "ci_gate_command": "make ci",
    "ci_gate_enabled": true,
    "git_commit_push_enabled": true,
    "git_revert_mode": "ask",
    "update_task_before_run": true,
    "notification": {
      "enabled": true,
      "notify_on_complete": true,
      "notify_on_fail": true,
      "notify_on_loop_complete": true,
      "sound_enabled": true,
      "suppress_when_active": true,
      "timeout_ms": 5000
    },
    "phase_overrides": {
      "phase1": {
        "runner": "codex",
        "model": "gpt-5.2",
        "reasoning_effort": "medium"
      },
      "phase2": {
        "runner": "kimi",
        "model": "kimi-code/kimi-for-coding"
      },
      "phase3": {
        "runner": "kimi",
        "model": "kimi-code/kimi-for-coding"
      }
    }
  }
}